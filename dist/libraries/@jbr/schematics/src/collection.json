{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "example": {
      "description": "Creates dynamic example wrapper component",
      "factory": "./example/index#createExample",
      "schema": "./example/schema.json"
    },
    "readme": {
      "description": "Creates composable readme template files to be used with gitdown",
      "factory": "./readme/index#createReadme",
      "schema": "./readme/schema.json"
    },
    "test": {
      "description": "Test",
      "factory": "./test/index#test",
      "schema": "./test/schema.json"
    },
    "comp-readme": {
      "description": "Generate a readme file from a component or directive file",
      "factory": "./comp-readme",
      "schema": "./comp-readme/schema.json"
    }
  }
}
